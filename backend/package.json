{
  "name": "jokes-api-backend",
  "version": "1.0.0",
  "description": "Backend API for Jokes Application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "npm run test:unit",
    "test:unit": "DB_SKIP_MIGRATION_TESTS=true jest --coverage",
    "test:all": "npm run db:test:start && jest --coverage --testTimeout=10000",
    "test:watch": "jest --watch",
    "db:test:start": "node test-setup.js --start",
    "db:test:stop": "node test-setup.js --stop",
    "db:test:remove": "node test-setup.js --remove",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:make": "knex migrate:make",
    "seed": "knex seed:run",
    "db:setup": "npm run migrate && npm run seed"
  },
  "keywords": [
    "jokes",
    "api",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "knex": "^3.1.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
